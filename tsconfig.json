{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "preserve",
    "types": [
      "node",
      "next/types"
    ],
    "paths": {
      "pdfjs-dist/legacy/build/pdf.worker.min?url": [
        "./node_modules/pdfjs-dist/types/legacy/build/pdf.worker.min.d.ts"
      ]
    },
    "module": "esnext",
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "allowJs": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
